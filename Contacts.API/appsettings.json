{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cosmos": {
    "Url": "<cosmosUri>",
    "Key": "<cosmosKey>",
    "Db": "<cosmosDbName>",
    "Container": "<cosmosDbDataContainerName>"
  },
  "Events": {
    "Ttl": "36000"
  }
}
